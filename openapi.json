{
    "openapi": "3.0.3",
    "info": {
      "title": "HIU API SDK",
      "description": "Health Information User API for handling consent and health information requests.",
      "version": "1.0.0"
    },
    "servers": [
      {
        "url": "https://example.com",
        "description": "Main production server"
      }
    ],
    "paths": {
      "/api/hiu/generate_consent_request": {
        "post": {
          "summary": "Generate a new consent request",
          "operationId": "generateConsentRequest",
          "responses": {
            "200": {
              "description": "Consent request successfully generated",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "message": {
                        "type": "string",
                        "example": "Consent request generated"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "/api/hiu/consents": {
        "get": {
          "summary": "List all consents",
          "operationId": "listConsents",
          "responses": {
            "200": {
              "description": "List of consents",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "consents": {
                        "type": "array",
                        "items": {}
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "/api/hiu/consents/{pk}": {
        "get": {
          "summary": "Retrieve a specific consent",
          "operationId": "retrieveConsent",
          "parameters": [
            {
              "name": "pk",
              "in": "path",
              "required": true,
              "schema": {
                "type": "integer"
              },
              "description": "Primary key of the consent"
            }
          ],
          "responses": {
            "200": {
              "description": "Consent retrieved",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "consent": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "integer"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "/api/hiu/consent_artefacts": {
        "get": {
          "summary": "List consent artefacts",
          "operationId": "listConsentArtefacts",
          "responses": {
            "200": {
              "description": "List of consent artefacts",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "consent_artefacts": {
                        "type": "array",
                        "items": {}
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "/api/hiu/consent_artefacts/{pk}": {
        "get": {
          "summary": "Retrieve a specific consent artefact",
          "operationId": "retrieveConsentArtefact",
          "parameters": [
            {
              "name": "pk",
              "in": "path",
              "required": true,
              "schema": {
                "type": "integer"
              },
              "description": "Primary key of the consent artefact"
            }
          ],
          "responses": {
            "200": {
              "description": "Consent artefact retrieved",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "consent_artefact": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "integer"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "/api/hiu/health-information/request": {
        "post": {
          "summary": "Request health information",
          "operationId": "requestHealthInformation",
          "responses": {
            "200": {
              "description": "Health information request submitted",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "message": {
                        "type": "string",
                        "example": "Health information request submitted"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "/api/hiu/health-information/receive": {
        "post": {
          "summary": "Receive health information",
          "operationId": "receiveHealthInformation",
          "responses": {
            "200": {
              "description": "Health information received",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "message": {
                        "type": "string",
                        "example": "Health information received"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "/your_gateway_callback_url_prefix/consent-requests/on-init": {
        "post": {
          "summary": "Handle gateway consent request initialization",
          "operationId": "gatewayConsentRequestOnInit",
          "responses": {
            "200": {
              "description": "Gateway consent request initialization handled",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "message": {
                        "type": "string",
                        "example": "Gateway consent request on init"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "/your_gateway_callback_url_prefix/consents/hiu/notify": {
        "post": {
          "summary": "Notify HIU of consent changes via gateway",
          "operationId": "gatewayConsentRequestNotify",
          "responses": {
            "200": {
              "description": "HIU notified of consent change",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "message": {
                        "type": "string",
                        "example": "Gateway consent request notify"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "/your_gateway_callback_url_prefix/consents/on-fetch": {
        "post": {
          "summary": "Fetch consents from gateway",
          "operationId": "gatewayConsentRequestOnFetch",
          "responses": {
            "200": {
              "description": "Consents fetched from gateway",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "message": {
                        "type": "string",
                        "example": "Gateway consent request on fetch"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "/your_gateway_callback_url_prefix/health-information/hiu/on-request": {
        "post": {
          "summary": "Handle gateway health information request",
          "operationId": "gatewayHealthInformationOnRequest",
          "responses": {
            "200": {
              "description": "Gateway health information request handled",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "message": {
                        "type": "string",
                        "example": "Gateway health information request"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
  